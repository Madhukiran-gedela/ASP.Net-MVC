@model List<Company.DomainModels.Product>
@{ 
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/admin_LayoutPage1.cshtml";
}

<h2>Categories</h2>
<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>         
    </tr>
    @foreach (var item in Model)
    {
        <tr>
        <td>@item.CategoryID</td>
        <td>@item.CategoryName</td>
        </tr>
    }
</table>

