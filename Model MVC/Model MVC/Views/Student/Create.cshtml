
@{
    ViewBag.Title = "Create";
}

<h2>Create Products</h2>
<form action="/Products/index" method="post">
    <div class="form-group">
    <label>Student Id</label>
    <input type="text" class="form-control"id="textId" placeholder="ProductId" name="StudentId">
    </div>
    <div class="form-group">
        <label>Student Name</label>
        <input type="text" class="form-control" id="textNam" placeholder="ProductName" name="StudentName">
    </div>
    <div class="form-group">
        <label>Street</label>
        <input type="text" class="form-control" id="textRate" placeholder="ProductRate" name="StudentStress">
    </div>
    <div class="form-group">
        <label>Dno</label>
        <input type="text" class="form-control" id="textRate" placeholder="ProductRate" name="StudentAddress">
    </div>
    <button class="btn btn-success">Submit</button>
</form>
